{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:new": "prisma migrate dev && prisma generate",
    "prisma:pull": "prisma db pull && prisma generate",
    "prisma:push": "prisma db push && prisma generate",
    "build:docker": "yarn build && docker build -t nextjs-docker .",
    "docker:run": "docker rm -f nextjs-docker && docker run --name nextjs-docker -d -p 8084:3000 nextjs-docker",
    "release:local": "pnpm build:docker && pnpm docker:run"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.5.2",
    "ioredis": "^5.3.2",
    "next": "14.0.2",
    "next-auth": "^4.24.5",
    "nodemailer": "^6.9.7",
    "prisma": "^5.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.69.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
